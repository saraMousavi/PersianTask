<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="ir.android.taskroom.ui.fragment.ProjectsFragment"
        tools:layout="@layout/projects_fragment" />

    <fragment
        android:id="@+id/navigation_dashboard"
        android:name="ir.android.taskroom.ui.fragment.ReminderFragment"
        tools:layout="@layout/reminder_fragment" />

    <fragment
        android:id="@+id/navigation_notifications"
        android:name="ir.android.taskroom.ui.fragment.CalenderFragment"
        tools:layout="@layout/calender_fragment" />
    <fragment
        android:id="@+id/navigation_setting"
        android:name="ir.android.taskroom.ui.fragment.SettingFragment"
        tools:layout="@layout/setting_fragment" />
</navigation>